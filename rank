function calcularSaldo(vitoria, derrota){
  let saldo = vitoria - derrota;
  let rank = "";

if ( saldo < 10) {
   rank = "Bronze"; 
} else if ( saldo > 20){
    rank = "Ferro";
} else if ( saldo > 40){
    rank = "Ouro";
} else if ( saldo > 60){
    rank = "Platina";
} else if ( saldo > 80){
    rank = "Diamente";

}

return rank;
}
let vitoria = 35;
let derrota = 10;

let resultado= calcularSaldo(vitoria, derrota);
console.log("O jogador esta no rank:", resultado);
